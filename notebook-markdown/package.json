{
	"enableProposedApi": true,
	"name": "notebook-markdown",
	"displayName": "notebook-markdown",
	"description": "Rendering Markdown inside a Notebook",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"notebookProvider": [
			{
				"viewType": "markdown",
				"displayName": "Markdown",
				"selector": [
					{
						"filenamePattern": "*.{md,markdown}"
					}
				]
			}
		],
		"commands": [
			{
				"command": "extension.showTestMarkdown",
				"title": "Show Sample Markdown as Notebook"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"download-api": "vscode-dts dev",
		"postdownload-api": "vscode-dts master",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"vscode-dts": "^0.3.1"
	}
}
